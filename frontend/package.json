{
  "name": "webassembly-rust-frontend",
  "version": "1.0.0",
  "description": "WebAssembly 2.0 + Rust 1.90 前端集成示例",
  "main": "src/index.js",
  "scripts": {
    "dev": "webpack serve --mode development",
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "clean": "rimraf dist",
    "serve": "http-server dist -p 8080 -o",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "format": "prettier --write src/**/*.{js,css,html}",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "analyze": "webpack-bundle-analyzer dist/static/js/*.js",
    "wasm:build": "cd .. && wasm-pack build --target web --out-dir frontend/pkg wasm",
    "wasm:build:examples": "cd .. && wasm-pack build --target web --out-dir frontend/pkg/basic examples/basic && wasm-pack build --target web --out-dir frontend/pkg/advanced examples/advanced && wasm-pack build --target web --out-dir frontend/pkg/performance examples/performance",
    "wasm:build:all": "npm run wasm:build && npm run wasm:build:examples",
    "start": "npm run wasm:build:all && npm run dev",
    "preview": "npm run build && npm run serve"
  },
  "keywords": [
    "webassembly",
    "rust",
    "wasm",
    "frontend",
    "javascript",
    "typescript",
    "webpack",
    "performance",
    "demo"
  ],
  "author": "WebAssembly Rust Team",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@types/jest": "^29.5.5",
    "babel-loader": "^9.1.3",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "html-webpack-plugin": "^5.5.3",
    "http-server": "^14.1.1",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "style-loader": "^3.3.3",
    "webpack": "^5.88.2",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "lodash": "^4.17.21"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/webassembly-rust.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/webassembly-rust/issues"
  },
  "homepage": "https://github.com/your-org/webassembly-rust#readme"
}
